{
  "connection": {
    "host": "localhost",
    "port": 6379,
    "db": 0,
    "maxRetriesPerRequest": 3,
    "enableReadyCheck": true,
    "connectTimeout": 10000,
    "keepAlive": 10000,
    "reconnectOnError": true,
    "retryStrategy": {
      "maxAttempts": 10,
      "initialDelay": 100,
      "maxDelay": 3000,
      "factor": 2
    }
  },
  "queues": {
    "defaultJobOptions": {
      "removeOnComplete": 100,
      "removeOnFail": 500,
      "attempts": 3,
      "backoff": {
        "type": "exponential",
        "delay": 2000
      }
    },
    "research": {
      "concurrency": 5,
      "rateLimit": {
        "max": 10,
        "duration": 60000
      },
      "stalledInterval": 30000,
      "maxStalledCount": 2
    },
    "webhook": {
      "concurrency": 10,
      "rateLimit": {
        "max": 50,
        "duration": 60000
      },
      "stalledInterval": 60000,
      "maxStalledCount": 3
    },
    "export": {
      "concurrency": 3,
      "stalledInterval": 120000,
      "maxStalledCount": 1
    }
  },
  "cache": {
    "ttl": {
      "default": 300,
      "session": 3600,
      "apiResponse": 60,
      "searchResults": 600
    },
    "maxKeys": 10000,
    "checkPeriod": 600
  },
  "pubsub": {
    "enableBuffering": true,
    "bufferLimit": 100,
    "flushInterval": 100
  }
}